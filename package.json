{
  "name": "lyra-discord-bot",
  "version": "1.1.0",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -w --outDir dist",
    "start": "ts-node --transpile-only src/bots/alpha/startAlphaBot.ts --env prod",
    "start:alpha-bot": "ts-node-dev -r tsconfig-paths/register --respawn src/bots/alpha/startAlphaBot.ts"
  },
  "dependencies": {
    "@discordjs/builders": "^0.7.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "axios": "^0.21.4",
    "better-sqlite3": "^7.4.3",
    "chalk": "^4.1.2",
    "csvtojson": "^2.0.10",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "ethereum-events": "^0.1.3",
    "ethers": "^5.4.7",
    "hardhat": "^2.6.6",
    "hardhat-ethers": "^1.0.1",
    "mongodb": "^4.3.0",
    "nullthrows": "^1.1.1",
    "path": "^0.12.7",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.4.0",
    "@types/node": "^17.0.8",
    "@types/node-fetch": "^2.5.10",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8"
  }
}
